#include <iostream>
using namespace std;
int main(){
    int arr[7]={4,7,2,8,12,10,6};
    int max=arr[0];
    int min=arr[0];
    for(int i=0;i<7;i++){
        if(max<arr[i]){
            max=arr[i];
        }
    }
    for(int i=0;i<7;i++){
        if(min>arr[i]){
            min=arr[i];
        }
    }
    cout<<"Maximum number: "<<max;
    cout<<"\nMinimum number: "<<min;
    return 0;
}
